/** *Author:Swfdong *All comments are written in Chinese *Aii滑块组件 */package aii.ui.controls {	import aii.ui.core.*;	public class RangeSlider extends Slider {		//私有属性		protected var _low:Number;		protected var _high:Number;		//构造		public function RangeSlider(args:Object):void {			super(args);		}		//当前较小值		public function get low():Number {			return _low;		}		public function set low(value:Number):void {			if (_low != value) {				_low = value;				invalidate(InvalidationType.SCROLL);			}		}		//当前较大值		public function get high():Number {			return _high;		}		public function set high(value:Number):void {			if (_high != value) {				_high = value;				invalidate(InvalidationType.SCROLL);			}		}	}}